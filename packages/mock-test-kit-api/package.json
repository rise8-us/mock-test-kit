    "@rise8/mock-test-kit-utils": "^1.5.2",
    "fastify": "4.7.0",
    "fastify-cli": "5.5.0",
    "fastify-plugin": "4.2.1",
    "fastify-sensible": "3.2.0",
    "graphql": "15.6.1",
    "jsonwebtoken": "8.5.1",
    "nodemon": "2.0.12"
  },
  "devDependencies": {
    "tap": "15.0.9"
  }
