{
  "name": "mock-test-kit-api",
  "version": "1.5.2",
  "description": "A Mock REST & GraphQL API",
  "author": "Joshua Nathaniel Miller <jmiller@rise8.us>",
  "private": true,
  "main": "app.js",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
